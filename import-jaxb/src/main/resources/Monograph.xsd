			<xs:sequence>
				<xs:element ref="UniqueIdentifier" minOccurs="0"/>
				<xs:element ref="MonographUnitIdentification" minOccurs="0"/>
				<xs:element ref="Title" minOccurs="0"/>
				<xs:element ref="Creator" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Contributor" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Publisher" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Printer" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="PhysicalDescription"/>
				<xs:element ref="ISBN" minOccurs="0"/>
				<xs:element ref="Language" maxOccurs="unbounded"/>
				<xs:element ref="Subject" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Keyword" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Accessibility" minOccurs="0"/>
				<xs:element ref="Notes" minOccurs="0"/>
				<xs:element ref="MonographComponentPart" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MonographPage" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="MonographUnitRepresentation" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="TechnicalDescription" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="Type" default="Volume">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="TableSupplement"/>
						<xs:enumeration value="TechnicalPlanSchemeSupplement"/>
						<xs:enumeration value="IllustrationSupplement"/>
						<xs:enumeration value="MapSupplement"/>
						<xs:enumeration value="Volume"/>
						<xs:enumeration value="OtherSupplement"/>
						<xs:enumeration value="Jacket"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonographUnitIdentification">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="MonographUnitNumber"/>
				<xs:element ref="MonographUnitName" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonographUnitNumber">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonographUnitName">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MonographUnitRepresentation">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="UniqueIdentifier" minOccurs="0"/>
				<xs:choice>
					<xs:element ref="UnitImage"/>
					<xs:element ref="UnitText"/>
				</xs:choice>
				<xs:element ref="TechnicalDescription" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="UnitImage">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="xs:anyType">
					<xs:attribute name="href" use="required" type="xs:string"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="UnitText">
		<xs:complexType>
			<xs:complexContent>
				<xs:restriction base="xs:anyType">
					<xs:attribute name="href" use="required" type="xs:string"/>
				</xs:restriction>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="ScanningDevice">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="ScanningParameters">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="OtherImagingInformation">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="TechnicalDescription">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="ScanningDevice"/>
				<xs:element ref="ScanningParameters" minOccurs="0"/>
				<xs:element ref="OtherImagingInformation" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
