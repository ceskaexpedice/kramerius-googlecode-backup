<processes>
	<process>
		<!-- identifikator  -->
		<id>generovani_pdf</id>
		<!-- jenom popis -->
		<description>Generovani pdf dokumentu</description>
		<!-- spoustena trida -->
		<mainClass>cz.incad.kramerius.processes.mock.MockLPProcess</mainClass>
		<!-- standardni vystup -->
		<standardOs>standard</standardOs>
		<!-- err vystup -->
		<errOs>err</errOs>
		<!--  Cesta ke kninovnam, ktere process potrebuje -->
		<lib>lib</lib>
	</process>
	
	<!-- Staticky export do pdf -->	
	<process>
		<id>static_export_CD</id>
		<description>Statick√Ω export do PDF na CD</description>
		<mainClass>cz.incad.kramerius.lp.PDFExport</mainClass>
		<standardOs>lrOut</standardOs>
		<errOs>lrErr</errOs>
		<lib>lr-runtime-lib</lib>
		<parameters>
			<parameter>pdfs_out</parameter>		
			<parameter>conf/kk.xml</parameter>		
		</parameters>
	</process>
	
</processes>