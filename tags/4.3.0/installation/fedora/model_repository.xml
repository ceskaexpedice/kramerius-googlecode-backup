        <!-- see http://www.openarchives.org/OAI/openarchivesprotocol.html#DefinitionsConcepts -->
        <!-- see http://www.openarchives.org/OAI/2.0/OAI-PMH.xsd -->
        <Identify>
          <repositoryName>FIXME:My Repository</repositoryName>
          <baseURL>FIXME:http://example.org/oaiprovider</baseURL>
          <protocolVersion>2.0</protocolVersion>
          <adminEmail>FIXME:someone@somwhere.org</adminEmail>
          <earliestDatestamp>FIXME:2005-04-01T12:00:00Z</earliestDatestamp>
          <deletedRecord>FIXME:persistent</deletedRecord>
          <granularity>YYYY-MM-DDThh:mm:ssZ</granularity>
        </Identify>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
  </foxml:datastream>
